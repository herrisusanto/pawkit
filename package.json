{
  "name": "pawkit",
  "version": "1.0.0",
  "main": "AppEntry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "lint": "eslint . --ignore-pattern '.storybook' --fix --ext js,ts,tsx --max-warnings 0",
    "format": "prettier --ignore-path '.prettierignore' --write '**/*.{js,ts,tsx}'",
    "test": "jest --passWithNoTests",
    "prepare": "husky",
    "storybook": "cross-env STORYBOOK_ENABLED=true expo start",
    "storybook-generate": "sb-rn-get-stories"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.js"
    ]
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.0.28",
    "@react-native-community/netinfo": "^11.3.1",
    "@tamagui/config": "^1.90.15",
    "@tamagui/core": "^1.90.15",
    "@tamagui/lucide-icons": "^1.90.15",
    "@tanstack/react-query": "^5.29.2",
    "aws-amplify": "^6.0.28",
    "expo-constants": "~15.4.5",
    "expo-crypto": "~12.8.1",
    "expo-dev-client": "~3.3.9",
    "expo-font": "~11.10.3",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.13",
    "jotai": "^2.7.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "react": "18.2.0",
    "react-hook-form": "^7.51.0",
    "react-native": "0.73.4",
    "react-native-get-random-values": "^1.11.0",
    "react-native-modal": "^13.0.1",
    "react-native-otp-entry": "^1.4.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-webview": "13.6.4",
    "tamagui": "^1.90.15"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-community/slider": "^4.5.0",
    "@storybook/addon-ondevice-actions": "^7.6.15",
    "@storybook/addon-ondevice-controls": "^7.6.15",
    "@storybook/react-native": "^7.6.15",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expo": "^50.0.8",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "^50.0.2",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run format",
      "npm run lint",
      "npm run test"
    ]
  },
  "private": true
}
